{
	"name": "wdl-debugger",
	"displayName": "WDL Debugger",
	"description": "Run and debug WDL code",
	"publisher": "broadinstitute",
	"repository": "https://github.com/broadinstitute/wdl-ide-vscode/wdl-debugger",
	"version": "0.0.1",
	"private": true,
	"engines": {
		"vscode": "^1.34.0"
	},
	"categories": [
		"Programming Languages",
		"Debuggers"
	],
	"activationEvents": [
		"onLanguage:wdl"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [{
			"command": "extension.helloWorld",
			"title": "Hello World"
		}]
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"package": "vsce package --yarn",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "yarn run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"@types/mocha": "^2.2.42",
		"@types/node": "^10.12.21",
		"tslint": "^5.12.1",
		"typescript": "^3.3.1",
		"vsce": "^1.62.0",
		"vscode": "^1.1.28"
	}
}
